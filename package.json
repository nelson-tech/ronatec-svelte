{
	"name": "ronatec",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev -p 8140",
		"build": "svelte-kit build",
		"start": "node dist/index.js -p 8140",
		"package": "svelte-kit package",
		"schema": "graphql-codegen --require dotenv/config --config ./packages/wp/graphql/getSchema.yaml",
		"gen": "graphql-codegen --config ./packages/wp/graphql/codegen.yaml",
		"preview": "svelte-kit preview",
		"prepare": "svelte-kit sync",
		"clean": "rm -rf dist .svelte-kit node_modules",
		"test": "playwright test",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@felte/reporter-svelte": "^1.1.2",
		"@kitql/client": "^0.5.7",
		"@kitql/graphql-codegen": "^0.5.2",
